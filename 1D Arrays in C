#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int sum=0,n,i;
scanf("%d",&n);
int *arr=(int*)malloc(n*sizeof(int));
for(i=0;i<n;i++)
{
 scanf("%d",&arr[i]);
   /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
}
for(i=0;i<n;i++)
{
    sum=sum+arr[i];
}
printf("%d",sum);
}
